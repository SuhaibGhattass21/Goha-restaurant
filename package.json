{
  "name": "goha",
  "version": "1.0.0",
  "main": "main.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/infrastructure/database/postgres/db.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/infrastructure/database/postgres/db.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/infrastructure/database/postgres/db.ts",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d src/infrastructure/database/postgres/db.ts",
    "schema:drop": "typeorm-ts-node-commonjs schema:drop -d src/infrastructure/database/postgres/db.ts",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "test:coverage:watch": "jest --coverage --watch --config jest.config.js",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:debug:watch": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch --config jest.config.js",
    "test:debug:coverage": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage --config jest.config.js",
    "test:debug:coverage:watch": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage --watch --config jest.config.js",
    "start": "tsc && node dist/main.js",
    "start:dev": "tsc && nodemon dist/main.js",
    "dev": "nodemon src/main.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "glob": "^11.0.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "pg": "^8.16.0",
    "pg-monitor": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.57"
  },
